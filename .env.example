# MyRunStreak.com Environment Variables
# Copy this file to .env and fill in your actual values
# NEVER commit .env to version control!

# SmashRun OAuth Credentials
# Get these from: https://smashrun.com/settings/api
SMASHRUN_CLIENT_ID=your-client-id-here
SMASHRUN_CLIENT_SECRET=your-client-secret-here
SMASHRUN_REDIRECT_URI=http://localhost:8000/callback

# DuckDB Configuration
# Local development: use local path
# Production: use S3 path
DUCKDB_PATH=./data/runs.duckdb
# DUCKDB_PATH=s3://myrunstreak-data/runs.duckdb

# AWS Configuration
AWS_REGION=us-east-2

# Application Settings
ENVIRONMENT=dev
LOG_LEVEL=INFO
